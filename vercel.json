{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/server-supabase.js" },
    { "src": "/uploads/(.*)", "dest": "/server-supabase.js" },
    { "src": "/styles.css", "dest": "/public/styles.css" },
    { "src": "/script.js", "dest": "/public/script.js" },
    { "src": "/config.js", "dest": "/public/config.js" },
    { "src": "/admin-functions.js", "dest": "/public/admin-functions.js" },
    { "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))", "dest": "/public/$1" },
    { "src": "/images/(.*)", "dest": "/public/images/$1" },
    { "src": "/(.*)", "dest": "/server-supabase.js" }
  ]
}


